<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="486.0" prefWidth="668.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.CustomerMainController">
   <children>
      <Label layoutX="49.0" layoutY="30.0" text="Customers">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TableView fx:id="customerMainTableView" layoutX="49.0" layoutY="73.0" prefHeight="335.0" prefWidth="587.0">
        <columns>
          <TableColumn fx:id="customerIdColumn" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="customerNameColumn" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="customerAddressColumn" prefWidth="75.0" text="Address" />
            <TableColumn fx:id="customerPostalColumn" prefWidth="75.0" text="Postal Code" />
            <TableColumn fx:id="customerPhoneColumn" prefWidth="75.0" text="Phone" />
            <TableColumn fx:id="customerCountryColumn" prefWidth="75.0" text="Country" />
            <TableColumn fx:id="customerDivisionColumn" prefWidth="75.0" text="Division" />
        </columns>
      </TableView>
      <Button fx:id="customersAddButton" layoutX="49.0" layoutY="425.0" mnemonicParsing="false" onAction="#customersAddButton" text="Add" />
      <Button fx:id="customersModifyButton" layoutX="139.0" layoutY="425.0" mnemonicParsing="false" onAction="#customersModifyButton" text="Modify" />
      <Button fx:id="customersBackButton" layoutX="584.0" layoutY="425.0" mnemonicParsing="false" onAction="#customersBackButton" text="Back" />
      <Button fx:id="customersDeleteButton" layoutX="239.0" layoutY="425.0" mnemonicParsing="false" onAction="#customersDeleteButton" text="Delete" />
   </children>
</AnchorPane>
